<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.osidocker.open.micro.draw.mapper.ActivePrizeStatisticsMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.osidocker.open.micro.draw.model.ActivePrizeStatistics">
        <id column="id" property="id" />
        <result column="class_id" property="classId" />
        <result column="type" property="type" />
        <result column="prize" property="prize" />
        <result column="partake_date" property="partakeDate" />
        <result column="prize_access" property="prizeAccess" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, class_id AS classId, type, prize, partake_date AS partakeDate, prize_access AS prizeAccess
    </sql>

</mapper>
